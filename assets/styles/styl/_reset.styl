/**
 * Eric Meyer's Reset CSS v2.0 (http://meyerweb.com/eric/tools/css/reset/)
 * http://cssreset.com
 */
html, body, div, span,
h1, h2, h3, h4, h5, h6, p,
a, em, img,
small, strong, i,
dl, dt, dd, ol, ul, li,
article, aside, canvas,
figure, figcaption, footer, header,
nav, section
  margin: 0;
  padding: 0;
  border: 0;
  font-size: 100%;
  font: inherit;
  vertical-align: baseline;

body
  line-height: 1;

ol, ul
  list-style: none;
